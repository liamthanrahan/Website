{
  "name": "personal-website",
  "version": "0.0.2",
  "description": "Liam Hanrahan's Personal Website",
  "scripts": {
    "clean": "rimraf public",
    "build:cp": "mkdir public | cp index.html public/index.html",
    "build": "npm run clean && npm run build:cp",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "webpack-dev-server --inline --content-base . --history-api-fallback",
    "start:prod": "webpack && node server.js",
    "lint": "eslint src",
    "deploy": "aws s3 sync public s3://liamhanrahan.com --acl public-read"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/liamthanrahan/personal-website.git"
  },
