  "author": "Liam Hanrahan <liam.hanrahan@arup.com> (http://github.com/liamthanrahan)",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/liamthanrahan/personal-website/issues"
  },
  "private": true,
  "homepage": "https://github.com/liamthanrahan/personal-website#readme",
