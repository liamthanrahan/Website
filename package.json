{
  "name": "liam-hanrahan-website",
  "version": "0.0.1",
  "description": "Liam Hanrahan's Personal Website",
  "scripts": {
    "clean": "rimraf public",
    "build:cp": "cp index.html public/index.html",
    "build": "npm run clean && npm run build:cp",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "webpack-dev-server --inline --content-base . --history-api-fallback",
    "start:prod": "webpack && node server.js",
    "lint": "eslint src",
    "deploy": "aws s3 sync public s3://liamhanrahansite --acl public-read --exclude \"*.json\" && aws s3 sync public s3://liamthanrahan --acl public-read --include \"*.json\" --content-type \"application/json\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/liamthanrahan/personal-website.git"
  },
  "keywords": [],
  "author": "Liam Hanrahan <liam.hanrahan@arup.com> (http://github.com/liamthanrahan)",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/liamthanrahan/personal-website/issues"
  },
  "private": true,
  "homepage": "https://github.com/liamthanrahan/personal-website#readme",
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "eslint": "^3.8.1",
    "eslint-plugin-react": "^6.4.1",
    "http-server": "^0.9.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.2",
    "rimraf": "^2.5.4",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.13.0"
  },
  "dependencies": {
    "autoprefixer": "^6.5.1",
    "babel": "^6.5.2",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "css-loader": "^0.25.0",
    "expect": "^1.20.2",
    "exports-loader": "^0.6.3",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
